6. ARQUITECTURA DE DATOS Y EVENTOS
6.1 MODELO DE DATOS MÃNIMO VIABLE
Copy-- ESTRUCTURA GOOGLE SHEETS / SUPABASE

-- Tabla: members
CREATE TABLE members (
  id SERIAL PRIMARY KEY,
  nombre VARCHAR(100) NOT NULL,
  telefono VARCHAR(20) UNIQUE NOT NULL,
  email VARCHAR(100),
  plan VARCHAR(50) DEFAULT 'basic',
  fecha_alta DATE DEFAULT CURRENT_DATE,
  consentimiento_wa BOOLEAN DEFAULT false,
  ultimo_checkin TIMESTAMP,
  dias_streak INTEGER DEFAULT 0,
  objetivo VARCHAR(100),
  preferencia_horario VARCHAR(20),
  estado VARCHAR(20) DEFAULT 'activo'
);

-- Tabla: checkins
CREATE TABLE checkins (
  id SERIAL PRIMARY KEY,
  member_id INTEGER REFERENCES members(id),
  timestamp TIMESTAMP DEFAULT NOW(),
  source VARCHAR(20) CHECK (source IN ('qr_cliente','profesor','recepcion')),
  class_id INTEGER,
  activity_type VARCHAR(30) DEFAULT 'entreno',
  staff_id INTEGER,
  duracion_minutos INTEGER
);

-- Tabla: payments
CREATE TABLE payments (
  id SERIAL PRIMARY KEY,
  member_id INTEGER REFERENCES members(id),
  monto DECIMAL(10,2) NOT NULL,
  fecha_vencimiento DATE NOT NULL,
  fecha_pago DATE,
  estado VARCHAR(20) DEFAULT 'pendiente',
  metodo_pago VARCHAR(30),
  intentos_cobro INTEGER DEFAULT 0,
  ultimo_recordatorio TIMESTAMP
);

-- Tabla: classes
CREATE TABLE classes (
  id SERIAL PRIMARY KEY,
  nombre VARCHAR(100) NOT NULL,
  instructor_id INTEGER,
  sala VARCHAR(50),
  fecha_hora TIMESTAMP,
  capacidad_maxima INTEGER,
  ocupacion_actual INTEGER DEFAULT 0,
  lista_espera INTEGER DEFAULT 0,
  estado VARCHAR(20) DEFAULT 'programada'
);

-- Tabla: feedback
CREATE TABLE feedback (
  id SERIAL PRIMARY KEY,
  member_id INTEGER REFERENCES members(id),
  class_id INTEGER REFERENCES classes(id),
  rating INTEGER CHECK (rating BETWEEN 1 AND 5),
  comentario TEXT,
  timestamp TIMESTAMP DEFAULT NOW(),
  categoria VARCHAR(50),
  respondido BOOLEAN DEFAULT false
);

-- Tabla: incidents
CREATE TABLE incidents (
  id SERIAL PRIMARY KEY,
  tipo VARCHAR(50) NOT NULL,
  prioridad VARCHAR(20) DEFAULT 'media',
  descripcion TEXT,
  sala_area VARCHAR(50),
  reportado_por INTEGER,
  asignado_a INTEGER,
  created_at TIMESTAMP DEFAULT NOW(),
  resolved_at TIMESTAMP,
  estado VARCHAR(20) DEFAULT 'abierto',
  resolucion TEXT
);
6.2 EVENTOS CRÃTICOS DEL SISTEMA
Copy// EVENTO: Check-in Registrado
{
  event_type: "checkin.registered",
  timestamp: "2024-01-15T19:00:00Z",
  data: {
    member_id: 123,
    source: "qr_cliente",
    class_id: 45,
    location: "entrada_principal"
  },
  triggers: [
    "update_ocupancy",
    "check_payment_status",
    "schedule_post_workout_message",
    "update_streak_counter"
  ]
}

// EVENTO: Pago Vencido
{
  event_type: "payment.overdue",
  timestamp: "2024-01-15T09:00:00Z",
  data: {
    member_id: 123,
    amount: 3200,
    days_overdue: 0,
    payment_id: 789
  },
  triggers: [
    "send_d0_reminder",
    "update_morosidad_dashboard",
    "flag_for_contextual_collection"
  ]
}

// EVENTO: Feedback Negativo
{
  event_type: "feedback.negative",
  timestamp: "2024-01-15T20:30:00Z",
  data: {
    member_id: 123,
    class_id: 45,
    rating: 2,
    comment: "mÃºsica muy alta",
    instructor_id: 12
  },
  triggers: [
    "analyze_pattern",
    "create_incident_if_recurring",
    "notify_instructor_privately",
    "schedule_followup"
  ]
}
6.3 FLUJOS DE INTEGRACIÃ“N N8N
Copy# FLUJO 1: Check-in QR
nombre: checkin_qr_flow
trigger: webhook:/checkin
nodos:
  1_validar_miembro:
    tipo: database
    query: SELECT * FROM members WHERE telefono = {{phone}}
    
  2_registrar_checkin:
    tipo: database
    query: INSERT INTO checkins (member_id, source, timestamp)
    
  3_verificar_pagos:
    tipo: database
    query: SELECT * FROM payments WHERE member_id AND estado = 'overdue'
    
  4_enviar_confirmacion:
    tipo: whatsapp
    template: checkin_confirmado
    params: [nombre, hora, clase]
    
  5_programar_encuesta:
    tipo: wait
    tiempo: 90_minutos
    then: enviar_encuesta_satisfaccion

# FLUJO 2: Cobranza Inteligente
nombre: cobranza_contextual
trigger: cron:daily_9am
nodos:
  1_identificar_morosos:
    tipo: database
    query: SELECT * FROM payments WHERE estado = 'overdue'
    
  2_segmentar_por_dias:
    tipo: switch
    casos:
      - dias = 0: enviar_recordatorio_d0
      - dias = 3: enviar_opciones_pago
      - dias = 7: enviar_urgencia
      - dias >= 14: derivar_humano
      
  3_check_contextual:
    tipo: database
    query: SELECT * FROM checkins WHERE member_id AND DATE(timestamp) = TODAY
    if_exists: programar_mensaje_post_entreno
