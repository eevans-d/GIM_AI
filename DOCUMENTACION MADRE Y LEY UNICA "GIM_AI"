CONSULTAS SQL OPERATIVAS
Copy-- Dashboard "Hoy"
WITH metricas_hoy AS (
  SELECT 
    -- Financiero
    (SELECT SUM(amount) FROM payments 
     WHERE DATE(paid_date) = CURRENT_DATE) as ingresos_hoy,
    
    (SELECT COUNT(DISTINCT member_id) FROM payments 
     WHERE estado = 'overdue') as morosos_activos,
    
    -- Operativo  
    (SELECT COUNT(*) FROM checkins 
     WHERE DATE(timestamp) = CURRENT_DATE) as checkins_hoy,
     
    (SELECT COUNT(*) FROM checkins 
     WHERE DATE(timestamp) = CURRENT_DATE 
     AND source = 'qr_cliente') as checkins_qr,
    
    -- OcupaciÃ³n actual
    (SELECT AVG(ocupacion_actual::float / capacidad_maxima) 
     FROM classes 
     WHERE DATE(fecha_hora) = CURRENT_DATE
     AND fecha_hora < NOW()) as ocupacion_promedio
)
SELECT * FROM metricas_hoy;

-- Morosidad con contexto
SELECT 
  m.nombre,
  m.telefono,
  p.monto,
  p.fecha_vencimiento,
  CURRENT_DATE - p.fecha_vencimiento as dias_vencido,
  CASE 
    WHEN c.timestamp > NOW() - INTERVAL '24 hours' 
    THEN 'EntrenÃ³ hoy - cobrar contextual'
    ELSE 'Sin check-in reciente'
  END as estrategia
FROM payments p
JOIN members m ON p.member_id = m.id
LEFT JOIN checkins c ON m.id = c.member_id
WHERE p.estado = 'overdue'
ORDER BY dias_vencido DESC;

-- AnÃ¡lisis ocupaciÃ³n para optimizaciÃ³n
WITH ocupacion_franja AS (
  SELECT 
    EXTRACT(HOUR FROM timestamp) as hora,
    COUNT(DISTINCT member_id) as asistentes,
    AVG(COUNT(DISTINCT member_id)) OVER (
      ORDER BY EXTRACT(HOUR FROM timestamp)
      ROWS BETWEEN 1 PRECEDING AND 1 FOLLOWING
    ) as promedio_movil
  FROM checkins
  WHERE timestamp > CURRENT_DATE - INTERVAL '21 days'
  GROUP BY hora
)
SELECT 
  hora,
  asistentes,
  promedio_movil,
  CASE
    WHEN promedio_movil < 50 THEN 'Valle - promocionar'
    WHEN promedio_movil > 90 THEN 'Pico - evaluar duplicar'
    ELSE 'Ã“ptimo'
  END as accion_sugerida
FROM ocupacion_franja
ORDER BY hora;
