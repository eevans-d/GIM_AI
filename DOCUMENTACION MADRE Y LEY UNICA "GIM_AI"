2.6 MÃ‰TRICAS Y FÃ“RMULAS OPERATIVAS
Definiciones Exactas de CÃ¡lculo
Copy-- MOROSIDAD (%)
morosidad = (
  SELECT SUM(amount) 
  FROM payments 
  WHERE status = 'overdue'
  AND due_date <= CURRENT_DATE
) / (
  SELECT SUM(amount)
  FROM payments
  WHERE due_date <= CURRENT_DATE
) * 100

-- DSO (Days Sales Outstanding)
dso = (
  SELECT SUM(amount)
  FROM payments
  WHERE status IN ('pending','overdue')
) / (
  SELECT AVG(daily_revenue)
  FROM (
    SELECT DATE(paid_date) as day,
           SUM(amount) as daily_revenue
    FROM payments
    WHERE status = 'paid'
    AND paid_date >= CURRENT_DATE - 30
    GROUP BY DATE(paid_date)
  )
)

-- OCUPACIÃ“N POR FRANJA (%)
ocupacion_franja = (
  SELECT COUNT(DISTINCT member_id)
  FROM checkins
  WHERE EXTRACT(HOUR FROM timestamp) BETWEEN 14 AND 16
  AND DATE(timestamp) = CURRENT_DATE
) / capacidad_total_franja * 100

-- NO-SHOW (%)
no_show = (
  SELECT COUNT(*)
  FROM reservations
  WHERE status = 'no_show'
  AND class_date >= CURRENT_DATE - 7
) / (
  SELECT COUNT(*)
  FROM reservations  
  WHERE status IN ('attended','no_show')
  AND class_date >= CURRENT_DATE - 7
) * 100
